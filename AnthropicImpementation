      "cell_type": "code",
      "source": [
        "import google.auth\n",
        "from google.colab import auth\n",
        "\n",
        "# Authenticate user with Google Colab\n",
        "auth.authenticate_user()\n",
        "\n",
        "# Get default credentials\n",
        "credentials, project_id = google.auth.default()\n",
        "\n",
